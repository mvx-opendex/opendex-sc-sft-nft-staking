{
  "steps": [
    {
      "step": "setState",
      "accounts": {
        "address:owner": {
          "nonce": "1",
          "balance": "0"
        }
      },
      "newAddresses": [
        {
          "creatorAddress": "address:owner",
          "creatorNonce": "1",
          "newAddress": "sc:staking"
        }
      ]
    },
    {
      "step": "scDeploy",
      "id": "deploy",
      "tx": {
        "from": "address:owner",
        "contractCode": "mxsc:../output/opendex-sc-sft-nft-staking.mxsc.json",
        "arguments": [
          "str:SFT-000000",
          "u64:0",
          "u64:99999999",
          "str:REWARD-000000",
          "address:fee",
          "5",
          "address:funder"
        ],
        "gasLimit": "50,000,000",
        "gasPrice": "0"
      },
      "expect": {
        "out": [],
        "status": "",
        "logs": [],
        "gas": "*",
        "refund": "*"
      }
    },
    {
      "step": "scQuery",
      "tx": {
        "to": "sc:staking",
        "function": "isAdmin",
        "arguments": ["address:owner"]
      },
      "expect": {
        "out": ["true"]
      }
    },
    {
      "step": "scQuery",
      "tx": {
        "to": "sc:staking",
        "function": "getPerformanceFeePercent",
        "arguments": []
      },
      "expect": {
        "out": ["5"]
      }
    },
    {
      "step": "scQuery",
      "tx": {
        "to": "sc:staking",
        "function": "getFunder",
        "arguments": []
      },
      "expect": {
        "out": ["address:funder"]
      }
    },
    {
      "step": "scQuery",
      "tx": {
        "to": "sc:staking",
        "function": "getStatus",
        "arguments": []
      },
      "expect": {
        "out": [
          {
            "01-staking_sft_collection_id": "nested:str:SFT-000000",
            "02-min_nonce_id": "u64:0",
            "03-max_nonce_id": "u64:99999999",
            "04-total_staked": "biguint:0",
            "05-reward_token": "nested:str:REWARD-000000",
            "06-staked_nft_collection_id": "u8:0",
            "07-reward_start_time": "u64:0",
            "08-reward_end_time": "u64:0",
            "09-reward_per_second": "biguint:0",
            "10-reward_per_token": "biguint:0",
            "11-fee_receiver": "address:fee",
            "12-performance_fee": "u32:5",
            "13-funder": "address:funder"
          }
        ]
      }
    }
  ]
}
